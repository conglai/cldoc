{
  "parser": "babel-eslint",
  "env": {
    "node": true,
    "es6": true
  },
  "rules": {
    "semi": 2,
    "quotes": [1, "single"],
    "no-eval": 1,
    "no-multi-str": 2,
    "no-new-func": 1,
    "no-new-object": 2,
    "no-new-require": 2,
    "no-redeclare": 2,
    "no-unreachable": 2,
    "no-with": 2,
    "default-case": 2,
    "eqeqeq": 2,
    "require-yield": 0,
    "indent":  [2, 2, {"SwitchCase": 1}]
  }
}
